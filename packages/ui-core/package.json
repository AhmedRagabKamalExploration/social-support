{
  "name": "@dge/ui-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/ui-core.d.ts",
      "import": "./dist/ui-core.js",
      "default": "./dist/ui-core.js"
    },
    "./index.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "storybook dev --port 6001 --no-open",
    "build": "vite build && pnpm run copy-css",
    "build:watch": "concurrently \"vite build --watch\" \"pnpm run copy-css\"",
    "copy-css": "cp -f src/index.css dist/style.css || echo 'CSS copy completed'",
    "lint": "eslint --max-warnings 0 .",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:update": "vitest -- -u",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:types": "vitest typecheck",
    "storybook": "storybook dev -p 6006",
    "clean": "rimraf node_modules/.cache storybook-static",
    "build-storybook": "storybook build",
    "preview-storybook": "serve storybook-static"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "tailwindcss": ">=4.0.0"
  },
